{
  "extends": ["taro/react", "standard"],
  "globals": {
    "wx": true,
    "definePageConfig": true
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "comma-dangle": [0],
    "no-undef": [0],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
